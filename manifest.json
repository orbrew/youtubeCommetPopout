{
    "name": "YoutubeCommentsPopout",
    "version": "0.1.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/watch*"],
            "js": ["content.js","jquery-3.6.0.min.js"],
            "css": ["youtubeComments.css"],
            "run_at": "document_end"
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}

